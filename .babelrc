{
  "presets": [
    [
      "env", {
        "modules": false,
        "targets": {
          "browsers": ["last 2 versions", "ie >= 11"]
        }
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-es2015-modules-commonjs",
    "array-includes",
    "transform-object-rest-spread"
  ]
}
